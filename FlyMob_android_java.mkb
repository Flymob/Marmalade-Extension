#!/usr/bin/env mkb
# Builder mkb file for the java portion of the FlyMob extension on android
platform JAVA

files
{
    (source/android)
    FlyMob.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "android/FlyMobSdk-1.7.0.jar"

option output-name=lib/android/FlyMob.jar
